@{
}

<h3>Display Person</h3>
<table>
    <tr>
        <td>
            First Name:
        </td>
        <td>
            @Model.firstName
        </td>
    </tr>
    <tr>
        <td>
           Last Name:
        </td>
        <td>
            @Model.lastName
        </td>
    </tr>
    <tr>
        <td>
            Age:
        </td>
        <td>
            @Model.age
        </td>
    </tr>
    <tr>
        <td>
            Email Address:
        </td>
        <td>
            @Model.emailAddress
        </td>
    </tr>
    <tr>
        <td>
            Password:
        </td>
        <td>
            @*Normally we do not show passwords, here we show them for educational purposes*@
            @Model.password  
        </td>
    </tr>
</table>
